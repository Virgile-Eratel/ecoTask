# Configuration de production pour EcoTask

# Domaine et ports
DOMAIN=ecotask.yourdomain.com
FRONTEND_PORT=80
FRONTEND_URL=http://ecotask.yourdomain.com

# Sécurité
JWT_SECRET=test

# Base de données
DATABASE_URL=file:./data/prod.db

# Backend
NODE_ENV=production
PORT=3002

# Monitoring (optionnel)
ENABLE_MONITORING=false
SENTRY_DSN=

# Logs
LOG_LEVEL=info

VITE_API_BASE_URL=http://localhost:3002
VITE_APP_NAME=EcoTask
VITE_APP_VERSION=1.0.0
